<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite parallel="none" name="Facebook - Web Auto UI">

    <listeners>
        <listener class-name="commons.MethodListener"/>
    </listeners>

<!--    <parameter name="endUserUrl" value="https://demo.nopcommerce.com/"></parameter>-->
    <parameter name="url" value="https://www.facebook.com/"/>

    <test name="Run on Chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.facebook.home.Level_20_ElementUndisplayed"/>
        </classes>
    </test>

</suite>